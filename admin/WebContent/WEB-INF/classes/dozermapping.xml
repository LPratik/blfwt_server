<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<configuration>
		<stop-on-errors>true</stop-on-errors>
		<date-format>dd/MM/yyyy HH:mm:ss</date-format>
		<wildcard>true</wildcard>
	</configuration>
	<mapping>
		<class-a>com.pron.blfwt.admin.model.MemberVO</class-a>
		<class-b>com.pron.blfwt.admin.entity.Member</class-b>
		<field
			custom-converter="com.pron.blfwt.admin.utils.MemberVOToMember">
			<a>birthDate</a>
			<b>birthDate</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.pron.blfwt.admin.entity.Member</class-a>
		<class-b>com.pron.blfwt.admin.dto.MemberDTO</class-b>
		<field
			custom-converter="com.pron.blfwt.admin.utils.MemberToMemberDTO">
			<a>birthDate</a>
			<b>birthDate</b>
		</field>
	</mapping>
</mappings>  